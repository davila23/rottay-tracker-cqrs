{
  "name": "cacodemon",
  "version": "0.1.0",
  "description": "CargoPanel's frontend",
  "scripts": {
    "build": "rm -rf dist && webpack --config webpack.prod.js",
    "clean": "rm -f .yarnrc && rm -f yarn-*.log && rm -f yarn.lock && rm -rf node_modules && yarn cache clean",
    "lint": "prettier-standard --lint",
    "locations": "./generate-locations.sh",
    "nuke": "yarn run clean && yarn && git checkout .yarnrc",
    "start": "webpack-dev-server --config webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "upgrade": "npx upgradeps"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.8.2",
    "@reduxjs/toolkit": "^1.3.6",
    "@sentry/browser": "^5.15.5",
    "@turf/bbox": "^6.0.1",
    "@turf/great-circle": "^5.1.5",
    "@turf/helpers": "^6.1.4",
    "@turf/midpoint": "^5.1.5",
    "antd": "^4.3.0",
    "axios": "^0.19.2",
    "date-fns": "^2.14.0",
    "graphql": "^15.0.0",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-map-gl": "^5.2.5",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-spring": "^8.0.27",
    "react-virtualized": "^9.21.2",
    "styled-components": "^5.1.1",
    "swagger-client": "^3.10.5",
    "typeface-gaegu": "^0.0.61",
    "typeface-ubuntu-mono": "^0.0.72",
    "urql": "^1.9.7"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@svgr/webpack": "^5.4.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.7",
    "prettier-standard": "^16.3.0",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "upgradeps": "^1.4.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "cargopanel.io",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "license": "UNLICENSED",
  "private": true
}
